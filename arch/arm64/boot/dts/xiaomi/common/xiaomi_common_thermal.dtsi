&pm8998_vadc {
	vph_pwr {
		label = "vph_pwr";
		reg = <ADC5_VPH_PWR>;
		qcom,pre-scaling = <1 1>;
	};

	vcoin {
		label = "vcoin";
		reg = <ADC5_VCOIN>;
		qcom,pre-scaling = <1 1>;
	};

	xo_therm {
		label = "xo_therm";
		reg = <ADC5_XO_THERM_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	cam_therm0 {
		label = "cam_therm0";
		reg = <ADC5_AMUX_THM1_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	cam_therm1 {
		label = "cam_therm1";
		reg = <ADC5_AMUX_THM2_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm0 {
		label = "pa_therm0";
		reg = <ADC5_AMUX_THM3_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm1 {
		label = "pa_therm1";
		reg = <ADC5_AMUX_THM4_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	quiet_therm {
		label = "quiet_therm";
		reg = <ADC5_AMUX_THM5_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm8998_adc_tm {
	#address-cells = <1>;
	#size-cells = <0>;

	io-channels = <&pm8998_vadc ADC5_VPH_PWR>,
	<&pm8998_vadc ADC5_XO_THERM_100K_PU>,
	<&pm8998_vadc ADC5_AMUX_THM1_100K_PU>,
	<&pm8998_vadc ADC5_AMUX_THM2_100K_PU>,
	<&pm8998_vadc ADC5_AMUX_THM3_100K_PU>,
	<&pm8998_vadc ADC5_AMUX_THM4_100K_PU>,
	<&pm8998_vadc ADC5_AMUX_THM5_100K_PU>,
	<&pm8998_vadc ADC5_GPIO1_100K_PU>; // beryllium-backlight

	vph_pwr {
		label = "vph_pwr";
		reg = <ADC5_VPH_PWR>;
		qcom,pre-scaling = <1 1>;
	};

	xo_therm {
		label = "xo_therm";
		reg = <ADC5_XO_THERM_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};

	cam_therm0 {
		label = "cam_therm0";
		reg = <ADC5_AMUX_THM1_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};

	cam_therm1 {
		label = "cam_therm1";
		reg = <ADC5_AMUX_THM2_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};

	pa_therm0 {
		label = "pa_therm0";
		reg = <ADC5_AMUX_THM3_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};

	pa_therm1 {
		label = "pa_therm1";
		reg = <ADC5_AMUX_THM4_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};

	quiet_therm {
		label = "quiet_therm";
		reg = <ADC5_AMUX_THM5_100K_PU>;
		qcom,ratiometric;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
	};
};

&thermal_zones {
	xo_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm8998_adc_tm ADC5_XO_THERM_100K_PU>;
		thermal-governor = "user_space";
		wake-capable-sensor;

		trips {
			active-config0 {
				temperature = <65000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	cam_therm0 {
		polling-delay-passive = <0>;
		polling-delay = <0>;
